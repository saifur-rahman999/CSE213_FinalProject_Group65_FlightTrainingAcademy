<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.cse213_finalproject_group65_flighttrainingacademy.FinanceAndEnrollmentOfficer.FEODashBoard"
            prefWidth="920" prefHeight="560">

    <Label text="Finance and Enrollment Officer Dashboard"
           layoutX="24" layoutY="20"
           style="-fx-font-size: 20; -fx-font-weight: bold;" />

    <!-- Row 1 -->
    <Button text="New Application" layoutX="24" layoutY="80" prefWidth="200" prefHeight="48"
            onAction="#openNewApplication"/>
    <Button text="Search Applicants" layoutX="244" layoutY="80" prefWidth="200" prefHeight="48"
            onAction="#openSearchApplicants"/>
    <Button text="Generate Invoice" layoutX="464" layoutY="80" prefWidth="200" prefHeight="48"
            onAction="#openGenerateInvoice"/>
    <Button text="Record Payment" layoutX="684" layoutY="80" prefWidth="200" prefHeight="48"
            onAction="#openRecordPayment"/>

    <!-- Row 2 -->
    <Button text="Track Outstanding" layoutX="24" layoutY="148" prefWidth="200" prefHeight="48"
            onAction="#openTrackOutstanding"/>
    <Button text="Enrollment Actions" layoutX="244" layoutY="148" prefWidth="200" prefHeight="48"
            onAction="#openEnrollmentActions"/>
    <Button text="Intake Summary" layoutX="464" layoutY="148" prefWidth="200" prefHeight="48"
            onAction="#openIntakeSummary"/>
    <Button text="Reminder Preview" layoutX="684" layoutY="148" prefWidth="200" prefHeight="48"
            onAction="#openReminderPreview"/>

    <!-- Sign Out -->
    <Button text="Sign Out" layoutX="780" layoutY="520" prefWidth="100" onAction="#signoutButtonOnAction"/>
</AnchorPane>
